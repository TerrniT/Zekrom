	"tokenColors": [
		{
			"scope": ["comment"],
			"settings": {
				"foreground": "$muted",
				"fontStyle": "italic"
			}
		},
		{
			"scope": ["constant"],
			"settings": {
				"foreground": "$pine"
			}
		},
		{
			"scope": [
				"constant.numeric",
				"constant.language",
				"constant.charcter.escape"
			],
			"settings": {
				"foreground": "$rose"
			}
		},
		{
			"scope": ["entity.name"],
			"settings": {
				"foreground": "$rose"
			}
		},
		{
			"scope": [
				"entity.name.section",
				"entity.name.tag",
				"entity.name.namespace",
				"entity.name.type"
			],
			"settings": {
				"foreground": "$foam"
			}
		},
		{
			"scope": ["entity.other.attribute-name", "entity.other.inherited-class"],
			"settings": {
				"foreground": "$iris",
				"fontStyle": "italic"
			}
		},
		{
			"scope": ["invalid"],
			"settings": {
				"foreground": "$love"
			}
		},
		{
			"scope": ["invalid.deprecated"],
			"settings": {
				"foreground": "$subtle"
			}
		},
		{
			"scope": ["keyword"],
			"settings": {
				"foreground": "$pine"
			}
		},
		{
			"scope": ["markup.inserted.diff"],
			"settings": {
				"foreground": "$foam"
			}
		},
		{
			"scope": ["markup.deleted.diff"],
			"settings": {
				"foreground": "$love"
			}
		},
		{
			"scope": ["meta.diff.range"],
			"settings": {
				"foreground": "$iris"
			}
		},
		{
			"scope": ["meta.tag", "meta.brace"],
			"settings": {
				"foreground": "$text"
			}
		},
		{
			"scope": ["meta.import", "meta.export"],
			"settings": {
				"foreground": "$pine"
			}
		},
		{
			"scope": "meta.directive.vue",
			"settings": {
				"foreground": "$iris",
				"fontStyle": "italic"
			}
		},
		{
			"scope": "meta.property-name.css",
			"settings": {
				"foreground": "$foam"
			}
		},
		{
			"scope": "meta.property-value.css",
			"settings": {
				"foreground": "$gold"
			}
		},
		{
			"scope": "meta.tag.other.html",
			"settings": {
				"foreground": "$subtle"
			}
		},
		{
			"scope": ["punctuation"],
			"settings": {
				"foreground": "$subtle"
			}
		},
		{
			"scope": ["punctuation.accessor"],
			"settings": {
				"foreground": "$pine"
			}
		},
		{
			"scope": ["punctuation.definition.string"],
			"settings": {
				"foreground": "$gold"
			}
		},
		{
			"scope": ["punctuation.definition.tag"],
			"settings": {
				"foreground": "$muted"
			}
		},
		{
			"scope": ["storage.type", "storage.modifier"],
			"settings": {
				"foreground": "$pine"
			}
		},
		{
			"scope": ["string"],
			"settings": {
				"foreground": "$gold"
			}
		},
		{
			"scope": ["support"],
			"settings": {
				"foreground": "$foam"
			}
		},
		{
			"scope": ["support.constant"],
			"settings": {
				"foreground": "$gold"
			}
		},
		{
			"scope": ["support.function"],
			"settings": {
				"foreground": "$love",
				"fontStyle": "italic"
			}
		},
		{
			"scope": ["variable"],
			"settings": {
				"foreground": "$rose",
				"fontStyle": "italic"
			}
		},
		{
			"scope": [
				"variable.other",
				"variable.language",
				"variable.function",
				"variable.argument"
			],
			"settings": {
				"foreground": "$text"
			}
		},
		{
			"scope": ["variable.parameter"],
			"settings": {
				"foreground": "$iris"
			}
		}
	]
}
